{
  "tasks": {
    "assistant-list": "deno run  --allow-read --allow-env --allow-net api/list.ts assistant",
    "assistant-create": "deno run  --allow-read --allow-env --allow-net api/create.ts assistant",
    "assistant-get": "deno run  --allow-read --allow-env --allow-net api/get.ts assistant",
    "assistant-update": "deno run  --allow-read --allow-env --allow-net api/update.ts assistant",
    "file-get": "deno run  --allow-read --allow-env --allow-net api/get.ts file"
  },
  "imports": {
    "@hono/hono": "jsr:@hono/hono@^4.6.5",
    "@std/assert": "jsr:@std/assert@1",
    "@std/cli": "jsr:@std/cli@^1.0.6",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.2",
    "@std/jsonc": "jsr:@std/jsonc@^1.0.1",
    "openai": "npm:openai@^4.68.1"
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "singleQuote": true,
    "proseWrap": "always",
    "semiColons": false,
    "exclude": ["tmp/"]
  }
}
